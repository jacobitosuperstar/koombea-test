             "input":{},
             "expected_output": {"status_code": 200}
         },
         {
             "test_description": "Test updating",
             "url": "/events/fe24f3a2-ae47-11ef-8513-2a70e04c3944",
             "http_method": "POST",
             "input":{
                "description": "Updated test"
             },
             "expected_output": {
                 "status_code": 200,
                 "event": {
                     "event_id": "fe24f3a2-ae47-11ef-8513-2a70e04c3944",
